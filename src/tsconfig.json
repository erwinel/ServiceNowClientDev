{
  "compileOnSave": true,
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "module": "none",
    "typeRoots": [
      "Scripts/typings/jquery",
      "Scripts/typings/angularjs",
      "Scripts/typings/bootstrap"
    ]
  },
  "exclude": [
    "node_modules",
    "wwwroot",
    "packages",
    "obj",
    "bin"
  ]
}
